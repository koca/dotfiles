# JS TS
alias yw="yarn workspace"

# Toggle Dark Mode
alias dd="dark-mode"

# Remove folders recursively
alias rmallnodemodules="find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
alias rmalldist="find . -name 'dist' -type d -prune -exec rm -rf '{}' +"

alias getlocalip="ipconfig getifaddr en0"


# Apps
alias cors='open -n /Applications/Google\ Chrome.app --args --user-data-dir="/var/tmp/Chrome dev session" --disable-web-security'
